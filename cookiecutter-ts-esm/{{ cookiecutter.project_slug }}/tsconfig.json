{
  "extends": "@tsconfig/node-{{ cookiecutter.__nodejs | nodejs_major }}-strictest-esm",
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
